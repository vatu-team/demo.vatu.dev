{
  "name": "vatu-demo",
  "version": "0.1.0",
  "description": "## Project URLS",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "test:cypress": "",
    "cypress:open": "cypress open --config-file tests/cypress.json",
    "styles:global": "sass public/app/themes/vatu-gp-child-theme/assets/sass/style.scss:public/app/themes/vatu-gp-child-theme/style.css --style=expanded --source-map --source-map-urls=relative",
    "styles:components": "sass public/app/themes/vatu-gp-child-theme/assets/sass/06-components/:public/app/themes/vatu-gp-child-theme/assets/css/ --style=expanded --source-map --source-map-urls=relative",
    "styles:editor": "sass public/app/themes/vatu-gp-child-theme/assets/sass/editor.scss:public/app/themes/vatu-gp-child-theme/editor.css --style=expanded --source-map --source-map-urls=relative",
    "styles:autoprefixer": "postcss public/app/themes/vatu-gp-child-theme/*.css public/app/themes/vatu-gp-child-theme/assets/css/*.css --use autoprefixer --replace --map",
    "styles:minify": "postcss public/app/themes/vatu-gp-child-theme/*.css public/app/themes/vatu-gp-child-theme/assets/css/*.css --use cssnano --replace --map",
    "styles": "run-s \"styles:global\" \"styles:components\" \"styles:editor\" \"styles:autoprefixer\" \"styles:minify\"",
    "styles:watch": "chokidar \"public/app/themes/vatu-gp-child-theme/assets/**/*.scss\" -c \"npm run styles\" --initial",
    "build": "run-p \"styles\"",
    "build:watch": "run-p \"styles:watch\""
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/vatu-team/demo.vatu.dev.git"
  },
  "author": "",
  "license": "ISC",
  "homepage": "https://github.com/vatu-team/demo.vatu.dev/#readme",
  "devDependencies": {
    "@bahmutov/cy-api": "2.2.3",
    "@percy/cli": "1.19.2",
    "@percy/cypress": "3.1.2",
    "@vatu/theme-build": "github:vatu-team/theme-build#0.0.5",
    "cypress": "12.5.1",
    "cypress-iframe": "1.0.1"
  }
}
